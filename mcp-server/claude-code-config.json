{
  "mcpServers": {
    "mcp-golive": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-go-live-service/mcp-server",
        "run",
        "python",
        "-m",
        "mcp_golive.server"
      ],
      "env": {
        "API_BASE_URL": "http://localhost:8080/api/v1"
      }
    }
  }
}
